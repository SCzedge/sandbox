<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.db">
	
	<select id="fetch" resultType="map">
		select *
		from tbl_test
	</select>	
	
	<insert id="submit">
		insert into tbl_test(rmk) values(#{rmk})
	</insert>
</mapper>
